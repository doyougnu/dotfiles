xkb_symbols "dyg-dvorak"
{
  include "us(dvorak)"            // includes the base US keys

  key.type = "ONE_LEVEL";
  key <RALT>  { [ Return    ] };
  key <CAPS>  { [ Control_R ] };
  key <RTRN>  { [ BackSpace ] };
  key <TAB>   { [ Escape    ] };
  key <BKSP>  { [ Tab       ] };
  key <RCTL>  { [ Caps_Lock ] };
};
